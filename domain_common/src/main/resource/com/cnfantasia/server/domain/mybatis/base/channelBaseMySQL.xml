    <!-- 根据序列号删除(主渠道表)信息。 -->
    <sql id="delete_channel">
		DELETE FROM t_channel WHERE f_id=#{_parameter}
    </sql>
    <delete id="delete_channel" parameterType="java.lang.String">
		<include refid="channelBase.delete_channel"/>
    </delete>
    
    <!-- 根据序列号批量删除(主渠道表)信息。 -->
    <delete id="delete_channel_Batch" parameterType="java.util.List">
		DELETE FROM t_channel WHERE 1=1 AND f_id IN 
		<foreach item="item" collection="list" open="(" separator="," close=")">
			#{item}
		</foreach>
    </delete>
    
</mapper>
