<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 用户，设备缴费次数初始化 -->
<mapper namespace="payCountInit">
	<!--更新用户缴费次数-->
	<update id="userPayCount">
		UPDATE t_user_pay_count SET f_num = 0;
	</update>

	<!--更新设备缴费次数-->
	<update id="devicePayCount">
		UPDATE t_device_pay_count SET f_num = 0;
	</update>


</mapper>
