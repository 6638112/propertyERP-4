<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 数据表 t_ebuy_product_shelf -->
<mapper namespace="ebuyProductShelf">
	<update id="delete_by_product_id" parameterType="java.math.BigInteger">
		update t_ebuy_product_shelf set f_sys0_del_state=1 where t_ebuy_product_id=#{productId}
	</update>
</mapper>
