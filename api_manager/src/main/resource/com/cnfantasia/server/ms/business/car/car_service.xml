	<bean id="carService" class="com.cnfantasia.server.ms.car.service.CarService">
		<property name="carDao" ref="carDao"></property>
		<property name="carNumListBaseDao" ref="carNumListBaseDao"></property>
		<property name="uuidManager" ref="uuidManager"></property>
	</bean>
	
</beans>  

